  <!-- LID Supersession LDD - Provenance LDD Built Class -->
  
  <!-- Reasons for Superseded LID 
  
  Replacement: The original LID no longer matches the product/entity.
  Duplication: An error, system issue, or oversight led to a duplicate LID.
  Merged: Multiple products/entities were combined into one to reduce redundancy.
  Obsolescence: The original product/entity is no longer in use.
  Policy: New policies require changes to the LID structure.
  
  -->
  
  <DD_Class>
    <name>SupersededLID</name>
    <version_id>1.0</version_id>
    <local_identifier>SupersededLID</local_identifier>
    <submitter_name>Steve Hughes</submitter_name>
    <definition>The Superseded LID LDD relates two LIDs, one that supersedes the other.</definition>
    <element_flag>true</element_flag>
    
    <DD_Association>
      <identifier_reference>title</identifier_reference>
      <reference_type>attribute_of</reference_type>
      <minimum_occurrences>1</minimum_occurrences>
      <maximum_occurrences>1</maximum_occurrences>
    </DD_Association>
    
    <DD_Association>
      <identifier_reference>local_id</identifier_reference>
      <reference_type>attribute_of</reference_type>
      <minimum_occurrences>1</minimum_occurrences>
      <maximum_occurrences>1</maximum_occurrences>
    </DD_Association>
    
    <DD_Association>
      <identifier_reference>description</identifier_reference>
      <reference_type>attribute_of</reference_type>
      <minimum_occurrences>0</minimum_occurrences>
      <maximum_occurrences>1</maximum_occurrences>
    </DD_Association>
    
    <DD_Association>
      <identifier_reference>Entity</identifier_reference>
      <reference_type>component_of</reference_type>
      <minimum_occurrences>1</minimum_occurrences>
      <maximum_occurrences>1</maximum_occurrences>
    </DD_Association>
  </DD_Class>
  
  <DD_Rule>
    <local_identifier>provattributes</local_identifier>
    <rule_context>//prov:Attributes</rule_context>
    <rule_assign>name="num_supersedes" value="count(//prov:attribute[text() = 'Supersedes'])"</rule_assign>
    <rule_assign>name="num_reasons" value="count(//prov:attribute[text() = 'Reason'])"</rule_assign>
    
    <DD_Rule_Statement>
      <rule_type>Assert</rule_type>
      <rule_test>$num_supersedes &gt; 0</rule_test>
      <rule_message>There must be at least one instance where prov:attribute = 'Supersedes'.</rule_message>
    </DD_Rule_Statement>
    
    <DD_Rule_Statement>
      <rule_type>Assert</rule_type>
      <rule_test>$num_reasons &gt; 0</rule_test>
      <rule_message>There must be at least one instance where prov:attribute = 'Reason'.</rule_message>
    </DD_Rule_Statement>
    
    <DD_Rule_Statement>
      <rule_type>Assert</rule_type>
      <rule_test>not(prov:attribute = 'Supersedes') or (starts-with(prov:value, 'urn:nasa:pds'))</rule_test>
      <rule_message>When prov:attribute is 'Supersedes', prov:value must start with 'urn:nasa:pds'.</rule_message>
    </DD_Rule_Statement>
    
    <DD_Rule_Statement>
      <rule_type>Assert</rule_type>
      <rule_test>not(prov:attribute = 'Reason') or (prov:value = ('Replacement', 'Duplication', 'Merged', 'Obsolescence', 'Policy'))</rule_test>
      <rule_message>When prov:attribute is 'Reason', prov:value must be one of these values: 'Replacement', 'Duplication', 'Merged', 'Obsolescence', 'Policy'.</rule_message>
    </DD_Rule_Statement>
    
  </DD_Rule>
  
</Ingest_LDD>
